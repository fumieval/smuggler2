{-# LANGUAGE NoImplicitPrelude #-}

module NoImplicitPrelude (
  export,
  version,
  useprelude,
  useprelude2 ) where

import Data.List ( (++), intercalate )
import Data.Version ( makeVersion )
import Prelude ( Semigroup((<>)) )

export = intercalate ", " ["Lorem", "ipsum", "dolor"]

version = makeVersion [1, 2, 3]

useprelude = "abc" <> "def"

useprelude2 = "efg" ++ "ghi"
